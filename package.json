{
  "scripts": {
    "dev": "npm run snapp && npm run tailwindcss",
    "snapp": "snapp build -W",
    "tailwindcss": "tailwindcss -i style/input.css -o style/output.css --watch",
    "build": " snapp build --minify && tailwindcss -i style/input.css -o style/output.css --minify"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.18"
  }
}
